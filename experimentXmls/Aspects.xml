<experiment>
	<name value="Aspects" />
	<runs value="3" />
	<playbackSeconds value="60" />
	<bitrate value="300000" />
	<seed value="123456" />
	<serverUploadBandwidth value="5560000" />
	<messageHeaderSize value="64" />
	<seed value="12345678" />
	<descriptions value="1" />
	<cycleLength value="1000" />
	<enableTextLogger />
	<framework value="PeerSimFramework" dropRate="0.01"
			minDelay="200" maxDelay="400" />


	<peerGroup size="200">
		<churnModel type="none" />

		<playerModule>
			<serverStartup value="10000" />
			<waitIfNoChunk />
			<bufferFromFirstChunk />
			<startupBuffering value="4000" />
			<streamWindowSize value="120" />
		</playerModule>

		<networkModule value="PeersimNode" >
			<behavior name="DNVPOverlayBehavior" timeoutcycle="5"
				expirationInterval="20" numOfNoncesToProduce="20" checkApproval="1.0"
				checkDisconnection="1.0" verificationsPerCycle="2" />
			<uploadBandwidthDistribution group="0">
				<distribution type="ConstantDistribution" value="5560000.0" />
			</uploadBandwidthDistribution>
		</networkModule>

		<streamingModule value="CoolStreaming">
			<overlayModule value="BSCAMPOverlay" c="0">
				<behavior name="DNVPOverlayBehavior" timeoutcycle="5"
					expirationInterval="20" numOfNoncesToProduce="20" checkApproval="1.0"
					checkDisconnection="1.0" verificationsPerCycle="2" />
				<ingredient name="BlackListBehavior" />
			</overlayModule>
			<!-- <behavior name = "OmissionDefenceBehavior" numOfCycles="10" thresholdForMisses="1" 
				roundsForDiffusion="3" roundsForAnswer="1" thresholdForCommittee="0.5"/> -->
		</streamingModule>
	</peerGroup>

	<peerGroup size="50">
		<streamingModule algorithm="CoolStreaming">
			<overlayModule algorithm="BSCAMPOverlay" c="0">
				<clusteringAlgorithm algorithm="FullCluster" />
				<behavior name="DNVPOverlayBehavior" timeoutcycle="5"
					expirationInterval="20" numOfNoncesToProduce="20" checkApproval="1.0"
					checkDisconnection="1.0" verificationsPerCycle="2" />
			</overlayModule>
			<behavior name="OmissionAttackBehavior" />
		</streamingModule>
	</peerGroup>
</experiment>
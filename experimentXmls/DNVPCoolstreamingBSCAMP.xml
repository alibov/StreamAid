<experiment>
	<name value="DNVPCoolstreamingBSCAMP" />
	<playbackSeconds value="30" />
	<runs value="1" />
	<churnModel type="sessionLengthAddOnFailure">
		<sessionLengthDistribution>
			<distribution type="LogNormalDistribution" mean="4.29"
				variance="1.28" />
		</sessionLengthDistribution>
	</churnModel>
	<!--<churnModel type="sessionLengthInterArrival"> <sessionLengthDistribution> 
		<distribution type="LogNormalDistribution" mean="4.29" variance="1.28" /> 
		</sessionLengthDistribution> <interArrivalDistribution> <distribution type="ParetoDistribution" 
		Xm="1.55" alpha="2.52" /> </interArrivalDistribution> </churnModel> -->
	<playerModule waitIfNoChunk="" bufferFromFirstChunk="true"
		serverStartup="10000" startupBuffering="2000" streamWindowSize="120" />


	<bitrate value="300000" />
	<seed value="123456" />
	<streamingModule algorithm="CoolStreaming" size="200">
		<overlayModule algorithm="BSCAMP" c="1">
			<ingredient name="DNVPOverlayIngredient" timeoutcycle="5"
				expirationInterval="20" numOfNoncesToProduce="20" checkApproval="1.0"
				checkDisconnection="1.0" verificationsPerCycle="2" />
		</overlayModule>
	</streamingModule>
</experiment>
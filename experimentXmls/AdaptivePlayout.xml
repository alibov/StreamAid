<experiment>
	<name value="AdaptivePlayout"/>
	<playbackSeconds value="300"/>
	<runs value="1"/>
	<uploadBandwidthDistribution>
		<distribution type="ConstantDistribution" value="5560000.0"/>
	</uploadBandwidthDistribution>
	<serverUploadBandwidth value="16680000"/> 
	<messageHeaderSize value="64"/>
	<seed value="12345678"/>
	<descriptions value="1"/>
	<cycleLength value="1000"/>
	<churnModel type="sessionLengthAddOnFailure">
		<sessionLengthDistribution>
			<distribution type="LogNormalDistribution" mean="4.29"
				variance="1.28" />
		</sessionLengthDistribution>
	</churnModel> 
<!-- 	<skipIfNoChunk /> -->
	<bufferFromFirstChunk />
	<startupBuffering value="3000"/>
	<serverStartup value="10000"/>
	<streamWindowSize value="120"/>
	<framework value="PeerSimFramework" dropRate="0.0" minDelay="200" maxDelay="400"  />
	<bitrate value="900000"/> <!-- 300 kbps -->
	<streamingAlgorithm algorithm="PushPullAlgorithm" size="100"> 
		<streamingAlgorithm algorithm="CoolStreaming" serverPush="false">
		 	<overlayAlgorithm H="10" L="4" S="12" algorithm="AraneolaOverlay" amountToSend="3" connect_timeout="2" disconnect_timeout="2" gossipDelay="6"/>
		  	<behavior name="ConstantBufferSizeAdaptivePlayoutBehavior" bufferSize="3" maxSpeedChange="0.1"/>  
		</streamingAlgorithm>
		<overlayAlgorithm algorithm="TreeBoneOverlay"
				stableCoefficient="0.3" queryTimeout="3" freeSlots="1"/>

	</streamingAlgorithm>
</experiment>

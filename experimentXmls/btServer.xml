<experiment>
	<name value="btserver" />
	<playbackSeconds value="60" />
	<runs value="1" />
	<churnModel type="none" />
	<startupBuffering value="2000" />
	<serverStartup value="10000" />
<!-- 	<enableTextLogger /> --> 
	<messageHeaderSize value="64" />
	<!-- 
		<churnModel type="sessionLengthAddOnFailure">
		<sessionLengthDistribution>
			<distribution type="LogNormalDistribution" mean="4.29"
				variance="1.28" />
		</sessionLengthDistribution>
	</churnModel> -->
	<bufferFromFirstChunk />
	<seed value="12345678" />
	<descriptions value="6" />
	<cycleLength value="1000" />
<!-- 	<framework value="NetworkFramework" role="server" port="8010"/>  -->
	<framework value="PeerSimFramework" dropRate="0.0" minDelay="200" maxDelay="400"  />
	<streamWindowSize value="120" />
	<bitrate value="500000" /> <!-- 500 kbps -->
	<streamingAlgorithm algorithm="PushMultipleSubstreamTree" sendMissingChunks="true" size="200">
		<overlayAlgorithm clubsForPeer="2" algorithm="BittorrentLiveOverlay" minInClubDownload="2" maxInClubDownload="3" fatherDropTimeout="2"/>
		<behavior name="WaitThenSkipPolicyBehavior" waitPeriod="2"/>
	</streamingAlgorithm>
</experiment>
